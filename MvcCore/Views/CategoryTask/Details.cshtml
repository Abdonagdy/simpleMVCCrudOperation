@using MvcCore.Models
@model CategoryTask
<div class="container text-center m-5">
	<div class="d-flex justify-content-around mb-4 ">
	<a class="btn btn-info pt-3 pb-3 ps-5 pe-5 m-3" asp-controller="CategoryTask" asp-action="New">
		Add Category
	</a>
	<a class="btn btn-info pt-3 pb-3 ps-5 pe-5 m-3 m-3" asp-controller="CategoryTask" asp-action="Index">
		Back To List
	</a>
	</div>
	<div class="row bg-success p-3 fs-6">
		<div class="col">
			Name
		</div>
	</div>
</div>
@if (Model!=null)
{
	
		<div class="container text-center m-5">
			<div class="row">
				<div class="col">
					@Model.category_Name
				</div>
			</div>
		</div>
	

}



